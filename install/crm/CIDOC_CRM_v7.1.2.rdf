Figure 19: Temporal entity A ends before the start of temporal entity B. Here A is shorter than B</rdfs:comment>
    <rdfs:domain rdf:resource="E2_Temporal_Entity" />
    <rdfs:range rdf:resource="E2_Temporal_Entity" />
    <rdfs:subPropertyOf rdf:resource="P182_ends_before_or_with_the_start_of" />
    <owl:inverseOf rdf:resource="P183i_starts_after_the_end_of" />
  </rdf:Property>
  <rdf:Property rdf:about="P183i_starts_after_the_end_of">
    <rdfs:label xml:lang="en">starts after the end of</rdfs:label>
    <rdfs:domain rdf:resource="E2_Temporal_Entity" />
    <rdfs:range rdf:resource="E2_Temporal_Entity" />
    <rdfs:subPropertyOf rdf:resource="P182i_starts_after_or_with_the_end_of" />
